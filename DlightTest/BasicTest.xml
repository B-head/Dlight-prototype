<?xml version="1.0" encoding="utf-8" ?>
<compile-test xmlns="CompileTestSchema.xsd">
  <case>
    <code>
      echo 10
    </code>
    <output>
      10
    </output>
  </case>
  
  <case>
    <code>
      echo 10 + 10
    </code>
    <output>
      20
    </output>
  </case>

  <case>
    <code>
      echo abc
    </code>
    <error>undefined-identifier</error>
  </case>
  
  <case>
    <code>
      echo { abc }
    </code>
  </case>
</compile-test>
