<?xml version="1.0" encoding="utf-8" ?>
<compile-test xmlns="CompileTestSchema.xsd">
  <case>
    <code>
      echo 10
    </code>
    <output>
      10
    </output>
  </case>
  
  <case>
    <code>
      echo 10 + 10
    </code>
    <output>
      20
    </output>
  </case>

  <case>
    <code>
      echo abc
    </code>
    <error key="undefined-identifier"/>
  </case>

  <case>
    <code>
      echo 1 + 2.0
    </code>
    <error key="impossible-calculate"/>
  </case>
  
  <case no-execute="true">
    <code>
      echo { abc }
    </code>
  </case>
</compile-test>
