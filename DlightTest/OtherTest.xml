<?xml version="1.0" encoding="utf-8" ?>
<compile-test xmlns="CompileTestSchema.xsd">
  <case>
    <code>
      echo 10
    </code>
    <output>10</output>
  </case>
  
  <case>
    <code>
      echo 10
      echo 20
      echo 30
    </code>
    <output>10 20 30</output>
  </case>
  
  <case>
    <code>
      echo 10 + 10
    </code>
    <output>20</output>
  </case>

  <case>
    <code>
      echo 1 + 2 * 3
    </code>
    <output>7</output>
  </case>

  <case>
    <code>
      echo 1 + 2.5
    </code>
    <output>3.5</output>
  </case>
  
  <case>
    <code>
      echo { 30 }
    </code>
    <error key="error-token"/>
    <error key="error-token"/>
  </case>
</compile-test>
