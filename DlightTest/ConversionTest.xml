<?xml version="1.0" encoding="utf-8" ?>
<compile-test xmlns="CompileTestSchema.xsd">
  <case>
    <code>
      var e:Binary64 := 25.5
      var f:Integer32 := e
      echo f
    </code>
    <output>25</output>
  </case>
  
  <case>
    <code>
      var e:Binary64 := 25.5
      echo Integer32(e)
    </code>
    <output>25</output>
  </case>

  <case>
    <code>
      echo 1 + 2.5
    </code>
    <output>3.5</output>
  </case>

  <case>
    <code>
      class A
      {
      var value := 11
      }
      
      class B
      {
      var value := 22
      routine from(a:A)
      {
      value := a.value
      return this
      }
      
      var a := A()
      var b:B := a
      echo b.value
      }
    </code>
    <output>11</output>
  </case>
</compile-test>
