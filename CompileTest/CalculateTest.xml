<?xml version="1.0" encoding="utf-8" ?>
<compile-test xmlns="CompileTestSchema.xsd">
  <case>
    <code>
      stdout := 10 + 10
    </code>
    <output>20</output>
  </case>

  <case>
    <code>
      stdout := 1 + 2 * 3
    </code>
    <output>7</output>
  </case>

  <case>
    <code>
      stdout := (1 + 2) * 3
    </code>
    <output>9</output>
  </case>

  <case>
    <code>
      stdout := 1 + 2.5
    </code>
    <output>3.5</output>
  </case>

  <case>
    <code>
      var a := 3
      a := a + 4
      stdout := a
    </code>
    <output>7</output>
  </case>

  <case>
    <code>
      var a := 3
      a += 4
      stdout := a
    </code>
    <output>7</output>
  </case>

  <case>
    <code>
      var a := 5
      a -= 3
      stdout := a
    </code>
    <output>2</output>
  </case>

  <case>
    <code>
      var a := 5
      3 =- a
      stdout := a
    </code>
    <output>-2</output>
  </case>

  <case>
    <code>
      var a := 3
      a += 4 * 5
      stdout := a
    </code>
    <output>23</output>
  </case>
</compile-test>
